<odoo>
    <data noupdate="0">

        <record id="product_category_retail" model="product.category">
            <field name="name">Retail</field>
        </record>

        <record id="reward_01" model="product.product">
            <field name="name">Reward service</field>
            <field name="standard_price">0</field>
            <field name="list_price">0</field>
            <field name="type">service</field>
            <field name="available_in_pos">True</field>
            <field name="categ_id" ref="product_category_retail"/>
            <field name="uom_id" ref="product.product_uom_unit"/>
            <field name="uom_po_id" ref="product.product_uom_unit"/>
            <field name="description">Reward service</field>
            <field name="default_code">Rs</field>
        </record>

        <record id="promotion_service_01" model="product.product">
            <field name="name">Promotion service</field>
            <field name="standard_price">0</field>
            <field name="list_price">0</field>
            <field name="type">service</field>
            <field name="available_in_pos">True</field>
            <field name="categ_id" ref="product_category_retail"/>
            <field name="uom_id" ref="product.product_uom_unit"/>
            <field name="uom_po_id" ref="product.product_uom_unit"/>
            <field name="description">PS</field>
            <field name="default_code">PS</field>
            <field name="taxes_id">[]</field>
        </record>

         <record id="voucher_service" model="product.product">
            <field name="name">Voucher service</field>
            <field name="standard_price">0</field>
            <field name="list_price">0</field>
            <field name="type">service</field>
            <field name="available_in_pos">True</field>
            <field name="categ_id" ref="product_category_retail"/>
            <field name="uom_id" ref="product.product_uom_unit"/>
            <field name="uom_po_id" ref="product.product_uom_unit"/>
            <field name="description">Voucher service</field>
            <field name="default_code">VC</field>
        </record>

        <record id="global_discount" model="product.product">
            <field name="name">Discount</field>
            <field name="standard_price">0</field>
            <field name="list_price">0</field>
            <field name="type">service</field>
            <field name="available_in_pos">True</field>
            <field name="categ_id" ref="product_category_retail"/>
            <field name="uom_id" ref="product.product_uom_unit"/>
            <field name="uom_po_id" ref="product.product_uom_unit"/>
            <field name="default_code">GD</field>
        </record>

        <record id="credit_service" model="product.product">
            <field name="name">Credit</field>
            <field name="standard_price">1</field>
            <field name="is_credit">True</field>
            <field name="list_price">100</field>
            <field name="type">service</field>
            <field name="available_in_pos">True</field>
            <field name="categ_id" ref="product_category_retail"/>
            <field name="uom_id" ref="product.product_uom_unit"/>
            <field name="uom_po_id" ref="product.product_uom_unit"/>
        </record>

        <record id="medical_insurance_service" model="product.product">
            <field name="name">Medical service</field>
            <field name="standard_price">0</field>
            <field name="list_price">0</field>
            <field name="type">service</field>
            <field name="available_in_pos">True</field>
            <field name="categ_id" ref="product_category_retail"/>
            <field name="uom_id" ref="product.product_uom_unit"/>
            <field name="uom_po_id" ref="product.product_uom_unit"/>
            <field name="description">Medical service</field>
            <field name="default_code">MS</field>
        </record>

    </data>
</odoo>
